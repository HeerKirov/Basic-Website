{
  "defaultSeverity": "warning",
  "extends": [

  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [true, "spaces", 4],
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-var-keyword": true,
    "await-promise": true,
    "array-type": [true,"array"]
  }
}
